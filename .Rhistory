library(tidyverse)
library(hrbrthemes)
library(wakefield)
students = 38
questions = 14
# finish
track_suspects = data.frame(rep = NA,
count = NA)
for(jj in 1:1000)
{
# initialize student dataframe
student = paste0("student ", 1:38)
df = tibble(q1 = sample(student, size = 38, replace = FALSE),
q2 = sample(student, size = 38, replace = FALSE),
q3 = sample(student, size = 38, replace = FALSE))
# start counter
counter = 0L
for(ii in 1:(length(student)-1))
{
# take a pair of students, going pair by pair
student1 = df[ii, 1] %>% unlist()
student2 = df[ii + 1, 1] %>% unlist()
# now check rank order of student1 and student2 in q2
rank1 = which(df$q2 == student1)
rank2 = which(df$q2 == student2)
# now check if next to each other
check_next = abs(rank1 - rank2)
# keep going if they match
if(check_next == 1)
{
# now check rank order of student1 and student2 in q3
rank1 = which(df$q3 == student1)
rank2 = which(df$q3 == student2)
# now check if next to each other
check_next = abs(rank1 - rank2)
if(check_next == 1)
{
# now check rank order of student1 and student2 in q3
rank1 = which(df$q4 == student1)
rank2 = which(df$q4 == student2)
# now check if next to each other
check_next = abs(rank1 - rank2)
if(check_next == 1)
{
# now check rank order of student1 and student2 in q3
rank1 = which(df$q5 == student1)
rank2 = which(df$q5 == student2)
# now check if next to each other
check_next = abs(rank1 - rank2)
if(check_next == 1)
{
counter = counter + 1
}
}
}
}
}
track_suspects[jj, ] <- c(jj, counter)
}
